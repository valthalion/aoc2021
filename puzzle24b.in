new_z: f(z, a, b, c, i) -> ((z//a)*26 if z%26+b!=i else z//a) + (i+c if z%26+b!=i else 0)


def new_z(z, a, b, c, i):
    if (z % 26) + b == i:
        return z // a
    else:
        return (z // a) * 26 + i + c

